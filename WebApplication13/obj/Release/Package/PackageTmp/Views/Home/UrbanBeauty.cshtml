    <script type="text/javascript">
    function loadData(tab) {
    // Show the loading overlay
    toggleOverlay();

    $.ajax({
        url: '@Url.Action("UrbanBeauty", "Home")', // Replace with your action
        type: 'GET',
        data: { tab: tab, bid: 99, accountId: '@Model.accountId' }, // Send the tab identifier
        success: function (data) {
            // Hide the loading overlay
            hideOverlay();

            // Update the content dynamically
            $('#dynamic-content').html(data);
        },
        error: function (xhr, status, error) {
            // Hide the loading overlay in case of error
            hideOverlay();
            alert('Failed to retrieve data: ' + error);
        }
    });
}

    </script>

    <script type="text/javascript">
        function switchActiveButton(button) {
            // Switch the active button
            document.getElementById("btn-massage").classList.remove("active-btn");
            document.getElementById("btn-beauty").classList.remove("active-btn");
            button.classList.add("active-btn");

            // Show the loading overlay when switching buttons
            toggleOverlay();
        }


    </script>


    <style>
        /* Your flat button styles */
        .flat-btn {
            display: inline-block;
            padding: 10px 20px;
            margin: 5px;
            font-size: 16px;
            font-weight: bold;
            text-align: center;
            text-decoration: none;
            outline: none;
            border: none;
            cursor: pointer;
            transition: background-color 0.3s, box-shadow 0.3s;
            border-radius: 5px;
        }

        .primary-btn {
            background-color: #3498db;
            color: white;
        }

            .primary-btn:hover {
                background-color: #2980b9;
                box-shadow: 0px 5px 10px rgba(0, 0, 0, 0.1);
            }

        .secondary-btn {
            background-color: #95a5a6;
            color: white;
        }

            .secondary-btn:hover {
                background-color: #7f8c8d;
                box-shadow: 0px 5px 10px rgba(0, 0, 0, 0.1);
            }

        .flat-btn:active {
            background-color: #2c3e50;
            box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.2) inset;
        }

        /* Media query for responsive design */
        @@media (max-width: 768px) {
            .flat-btn {
                width: 100%;
                margin: 10px 0;
            }
        }

        /* Active button style */
        .active-btn {
            background-color: #3498db !important;
            color: white;
        }

        /* Non-active (grey) button */
        .flat-btn:not(.active-btn) {
            background-color: #95a5a6;
        }
    </style>

</head>

