                </div>
                <div class="col-lg-10">

                </div>
                <div class="col-lg-2">
                    <button id="memberEditBtn" type="button" class="btn btn-primary">Edit information</button>
                    <button id="deleteBtn" type="button" class="btn btn-danger">Delete</button>

                    @*<button type="button" class="memberEditBtn" data-href=@Model.MemberList[0].Id>Edit information</button>*@
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
