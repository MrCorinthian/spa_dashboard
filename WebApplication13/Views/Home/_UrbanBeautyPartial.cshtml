                        <tr>
                            <th>Massage Type Name</th>
                            <th>Total Pax</th>
                            <th>Total Sale</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.finalSaleForEach)
                        {
