    <script type="text/javascript">
        document.addEventListener('DOMContentLoaded', function () {
            // Get the table rows
            var memBtn = document.getElementById("memberEditBtn").addEventListener("click", function (e) {

                // Get the href value associated with the row
                var href = @Model.MemberList[0].Id;
                console.log(href);

                // Navigate to the specified page
                window.location.href = "EditMemberDetail" + '?MemberId=' + encodeURIComponent(href)+"&Mode=Edit";
            });
        });

    </script>

    <script type="text/javascript">
        // Get the data from the query string
        var _data = new URLSearchParams(location.search).get("MemberId");

        // Use the data
        console.log(_data);
    </script>

    <script type="text/javascript">
        document.addEventListener('DOMContentLoaded', function () {
            $("#deleteBtn").click(function () {
                var result = confirm("Are you sure you want to delete this record?");
                if (result) {
                    var id = _data;
                    $.ajax({
                        type: "POST",
                        url: "/Home/DeleteMember",
                        data: { id: id },
                        success: function (response) {
                            if (response === "Success") {
                                alert("Data deleted successfully!");
                                window.location.href = "Member";
                            } else {
                                alert("Error deleting data. Please try again.");
                            }
                        }
                    });
                }
            });
        });
    </script>
    <script type="text/javascript">
        function navigateToOtherPage() {
            document.getElementById("overlay").style.display = "block";
        }
    </script>

    <script type="text/javascript">

        function navigationSuccess() {
            document.getElementById("overlay").style.display = "none";
        }

    </script>

