                                    @*<tr>
                                        <td>No.</td>
                                        <td class="center">@item.Id</td>
                                    </tr>*@
                                    <tr>
                                        <td style="background-color:#F0F0F0">Member no.</td>
                                        <td class="center">@item.MemberNo</td>
                                    </tr>
                                    <tr>
