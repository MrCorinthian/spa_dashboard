                    <!--<div class="panel panel-default">-->
                    <!--<div class="panel-heading">
                        VIP Member
                    </div>-->
                    <!-- /.panel-heading -->
                    <div class="panel-body" style="overflow-x:auto;">
                        <table width="100%" class="table table-bordered table-hover" id="memberTable">
                            <thead>
                                <tr style="background-color:#F0F0F0">
                                    <th>No.</th>
                                    <th>Group name</th>
                                    <th>Group show name</th>
                                    <th>Priviledge</th>
                                    <th>Active status</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.MemberGroupList)
                                {
                                    <tr class="memberGroupRow" data-href=@item.MemberGroupId>
                                        <td class="center">@item.MemberGroupId</td>
                                        <td class="center">@item.MemberGroupName</td>
                                        <td class="center">@item.MemberGroupShowName</td>
                                        <td class="center">@item.MemberPriviledgeName</td>
                                        <td class="center">@item.Status</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                        <!-- /.table-responsive -->
                    </div>
                    <!-- /.panel-body -->
                    <!--</div>-->
                    <!-- /.panel -->
