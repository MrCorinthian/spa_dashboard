@model WebApplication13.Models.HeaderWithBeauty

<div class="col-lg-12">
    <br />
    <h4>Average  :  @Html.DisplayFor(model => model.strAverage)</h4>
    <br />
    <div class="radiofilter">
        Select report type :
        <label for="chkDaily">
            <input type="radio" id="chkDaily" onclick="ShowHideDiv(this)" checked />
            Daily
        </label>
        <label for="chkMonthly">
            <input type="radio" id="chkMonthly" onclick="ShowHideDiv2(this)" />
            Monthly
        </label>
        <label for="chkYearly">
            <input type="radio" id="chkYearly" onclick="ShowHideDiv3(this)" />
            Yearly
        </label>
    </div>
    <div class="select-datetime" id="selectdatetime" style="display:block">
        @using (Html.BeginForm("UrbanBeauty", "Home", FormMethod.Get))
        {

            <fieldset>
                <input type="hidden" name="bid" value=@Model.bid />
                Transaction start Date/Time : @Html.DropDownList("accountId", Model.listAllAccounts) <input type="submit" value="Submit" onclick="toggleOverlay()" />
            </fieldset>

        }

    </div>
    <div class="select-month" id="selectmonth" style="display:none">
        @using (Html.BeginForm("UrbanBeauty", "Home", FormMethod.Get))
        {

            <fieldset>
                <input type="hidden" name="bid" value=@Model.bid />
                Month and Year : @Html.DropDownList("monthNo", Model.listAllMonths) @Html.DropDownList("yearNo", Model.listAllYears) <input type="submit" value="Submit" onclick="toggleOverlay()" />
            </fieldset>

        }

    </div>
    <div class="select-year" id="selectyear" style="display:none">
        @using (Html.BeginForm("UrbanBeauty", "Home", FormMethod.Get))
        {

            <fieldset>
                <input type="hidden" name="bid" value=@Model.bid />
                Year : @Html.DropDownList("yearNo", Model.listAllYears) <input type="submit" value="Submit" onclick="toggleOverlay()" />
            </fieldset>

        }

    </div>
    <p></p>
    <br />

</div>
<!-- /.row -->
<!-- Initial Money -->
