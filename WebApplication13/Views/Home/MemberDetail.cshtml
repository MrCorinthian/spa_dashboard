                <a class="navbar-brand" href="#">Spa System Admin Panel</a>
            </div>
            <!-- /.navbar-header -->

            <ul class="nav navbar-top-links navbar-right">
                @*<li class="dropdown">
                    <a href="@Url.Action("UrbanThree", "Home",new { cmd = "logout" })"><i class="fa fa-power-off fa-2x"></i></a>
                </li>*@
                <!-- /.dropdown -->
