                                    </tr>
                                    <tr>
                                        <td style="background-color:#F0F0F0">Line id</td>
                                        <td class="center">@item.LineId</td>
                                    </tr>
                                    <tr>
                                        <td style="background-color:#F0F0F0">VIP start date</td>
                                        <td class="center">@item.VipStart</td>
                                    </tr>
                                    <tr>
                                        <td style="background-color:#F0F0F0">VIP expire date</td>
                                        <td class="center">@item.VipExpire</td>
                                    </tr>
                                    <tr>
